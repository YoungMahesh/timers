{
  "name": "timers",
  "version": "2.2.0",
  "private": true,
  "engines": {
    "bun": ">=1.3.3"
  },
  "scripts": {
    "build": "bunx dotenvx run -f ./.env -- bun --bun next build",
    "db:push": "bunx dotenvx run -f ./.env -- prisma db push",
    "db:studio": "bunx dotenvx run -f ./.env -- prisma studio",
    "dev": "bunx dotenvx run -f ./.env -- bun --bun next dev",
    "format": "bunx prettier -w src/ README.md",
    "postinstall": "bunx dotenvx run -f ./.env -- bun --bun prisma generate",
    "start": "bunx dotenvx run -f ./.env -- bun --bun next start"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.26.2",
    "@dotenvx/dotenvx": "^1.51.4",
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^5.1.1",
    "@t3-oss/env-nextjs": "^0.7.0",
    "@tanstack/react-query": "^4.32.6",
    "@trpc/client": "^10.37.1",
    "@trpc/next": "^10.37.1",
    "@trpc/react-query": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "install": "^0.13.0",
    "next": "^14.0.0",
    "react": "18.2.0",
    "react-datetime-picker": "^5.5.3",
    "react-dom": "18.2.0",
    "react-toastify": "^9.1.3",
    "superjson": "^1.13.1",
    "sweetalert2": "^11.9.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.9.4",
    "eslint": "^8.47.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.1",
    "prisma": "^5.1.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "ct3aMetadata": {
    "initVersion": "7.23.1"
  }
}
